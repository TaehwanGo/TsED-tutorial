{
  "name": "ts-ed-tutorial",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && yarn run tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@tsed/ajv": "^7.0.0-beta.10",
    "@tsed/common": "^7.0.0-beta.10",
    "@tsed/core": "^7.0.0-beta.10",
    "@tsed/di": "^7.0.0-beta.10",
    "@tsed/engines": "^7.0.0-beta.10",
    "@tsed/exceptions": "^7.0.0-beta.10",
    "@tsed/json-mapper": "^7.0.0-beta.10",
    "@tsed/logger": "^6.1.1",
    "@tsed/logger-file": "^6.1.1",
    "@tsed/passport": "^7.0.0-beta.10",
    "@tsed/platform-cache": "^7.0.0-beta.10",
    "@tsed/platform-exceptions": "^7.0.0-beta.10",
    "@tsed/platform-express": "^7.0.0-beta.10",
    "@tsed/platform-log-middleware": "^7.0.0-beta.10",
    "@tsed/platform-middlewares": "^7.0.0-beta.10",
    "@tsed/platform-params": "^7.0.0-beta.10",
    "@tsed/platform-response-filter": "^7.0.0-beta.10",
    "@tsed/platform-views": "^7.0.0-beta.10",
    "@tsed/schema": "^7.0.0-beta.10",
    "@tsed/swagger": "^7.0.0-beta.10",
    "ajv": "^8.11.0",
    "barrelsby": "^2.3.4",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "method-override": "^3.0.0",
    "passport": "^0.5.2"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.20.8",
    "@tsed/cli-plugin-jest": "3.20.8",
    "@tsed/cli-plugin-passport": "3.20.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.25",
    "@types/passport": "^1.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  }
}
